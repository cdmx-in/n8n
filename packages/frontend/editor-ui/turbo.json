{
	"extends": ["//"],
	"tasks": {
		"fetch-popularity": {
			"cache": true,
			"outputs": [".build/node-popularity.json"],
			"inputs": ["scripts/fetch-node-popularity.mjs"],
			"env": ["POPULARITY_CACHE_WEEK"]
		},
		"build": {
			"dependsOn": ["^build", "fetch-popularity"],
			"outputs": ["dist/**"]
		}
	}
}
